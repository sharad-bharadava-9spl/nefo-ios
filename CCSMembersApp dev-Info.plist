	<array>
		<string>ProductSans-ThinItalic.ttf</string>
		<string>ProductSans-Thin.ttf</string>
		<string>ProductSans-Regular.ttf</string>
		<string>ProductSans-MediumItalic.ttf</string>
		<string>ProductSans-Medium.ttf</string>
		<string>ProductSans-LightItalic.ttf</string>
		<string>ProductSans-Light.ttf</string>
